{
	"name": "sql.js",
	"version": "1.0.0",
	"description": "sqleet library with support for opening and writing databases, prepared statements, and more. This SQLite library is in pure javascript (compiled with emscripten).",
	"keywords": [
		"sql",
		"sqlite",
		"sqleet",
		"stand-alone",
		"relational",
		"database",
		"RDBMS",
		"data",
		"query",
		"statement",
		"emscripten",
		"wasm"
	],
	"license": "MIT",
	"main": "./dist/sqleet.js",
	"scripts": {
		"build": "webpack && make && npm run build-worker",
		"build-worker": "webpack --config webpack.worker.config.js",
		"clean": "make clean",
		"create-worker-scripts": "cd build && node createWorkerScript.js",
		"build-then-run-server": "yarn run build && cd examples && python ./start_local_server.py",
		"test": "npm run test-asm && npm run test-asm-debug && npm run test-wasm && npm run test-wasm-debug && npm run test-asm-memory-growth",
		"test-asm": "node test/all.js asm",
		"test-asm-debug": "node test/all.js asm-debug",
		"test-asm-memory-growth": "node test/all.js asm-memory-growth",
		"test-wasm": "node test/all.js wasm",
		"test-wasm-debug": "node test/all.js wasm-debug"
	},
	"homepage": "http://github.com/pwnsdx/sqleet.js",
	"repository": {
		"type": "git",
		"url": "http://github.com/pwnsdx/sqleet.js.git"
	},
	"bugs": {
		"url": "https://github.com/pwnsdx/sqleet.js/issues",
		"email": "sabri@riseup.net"
	},
	"devDependencies": {
		"test": ">=0.6",
		"tiny-worker": "^2.1.2",
		"ts-loader": "^6.0.4",
		"typescript": "^3.5.3",
		"webpack-cli": "^3.3.6"
	},
	"dependencies": {
		"@types/emscripten": "^1.38.0",
		"@types/node": "^12.7.1",
		"@types/sharedworker": "^0.0.28",
		"webpack": "^4.39.2"
	}
}
